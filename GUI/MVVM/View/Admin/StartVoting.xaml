<Window x:Class="muzickiKatalog.GUI.MVVM.View.Admin.StartVoting"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:muzickiKatalog.GUI.MVVM.View.Admin"
        mc:Ignorable="d"
        Title="StartVoting" Height="450" Width="800">
    <Grid>
        <ContentControl Template="{StaticResource ResourceKey=pozadina}"/>
        <Label Content="VOTING" FontSize="40" Style="{StaticResource ResourceKey=naslov}" Height="65"/>

        <Label Content="Start date: "
               Style="{StaticResource ResourceKey=naslov}" 
               Margin="-550,100,0,0"/>
        <TextBox Height="40"
                 Width="200"
                 Margin="-140,-180,0,0"/>

        <Label Content="End date: "
               Style="{StaticResource ResourceKey=naslov}" 
               Margin="-550,250,0,0"/>
        <TextBox Height="40"
                 Width="200"
                 Margin="-140,120,0,0"/>

    </Grid>
</Window>
